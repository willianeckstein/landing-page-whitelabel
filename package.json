    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
